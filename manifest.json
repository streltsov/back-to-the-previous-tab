{
  "manifest_version": 2,
  "name": "Back to the Previous Tab",
  "version": "1.0",

  "description": "Navigate back to the previous tab instantly with Ctrl+6",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}
